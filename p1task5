import java.util.Scanner;
import java.util.Arrays;
public class Main{
public static void main(String[]args){
    Scanner in = new Scanner(System.in);

    System.out.println("Введите 10 элементов массива:");
    int [] mas5 = new int[10];
    for (int i = 0; i < 10; i++){
        mas5[i] = in.nextInt();
    }
    Arrays.sort(mas5);
    for (int i = 0; i < 10; i++){
        System.out.println(mas5[i]);
    }
}
}
