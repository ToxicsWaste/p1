import java.util.Scanner ;
import java.util.ArrayList ;


public class Main {
    public static void main(String[] args) {
        int list[]={1, 2, 3, 4, 5,6, 7, 8, 9, 0};
        int sum = 0;
        for(int i=0;i< list.length; i++){
            sum = sum+list[i];
        }
        double x = (double) sum/ list.length;
        System.out.println(x);

    }
}
